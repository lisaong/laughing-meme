extend layout
block content
    h3= message
    section.main-content
        p=input
    section#one.wrapper
        table
            tr
                th Score
                th TSC Name
                th Category
                th Level
                th Description
                th Knowledge
                th Abilities
            for item in results
                //- indentation matches the top level tr above
                tr
                    td= item.score
                    td= item.tsc.name
                    td= item.tsc.category
                    td= item.tsc.level
                    td= item.tsc.description
                    td= item.tsc.knowledge
                    td= item.tsc.abilities
